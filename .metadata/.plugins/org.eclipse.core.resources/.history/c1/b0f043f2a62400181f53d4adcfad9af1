package es.hackerrank.day13;

 class MyBook extends Book{
	 private int price;

	MyBook(String title, String author, int price) {
		super(title, author);
		this.price=price;
	}

	@Override
	void display() {
		 System.out.println(
					"Title: " + title +
					"\nAuthor: " + author 
				+ 	"\nPrice: " + price); 
		
	}

}
