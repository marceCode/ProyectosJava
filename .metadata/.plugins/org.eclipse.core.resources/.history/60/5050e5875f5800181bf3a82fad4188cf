package es.com.java8tutorial.Java8tutorial;

public class DefaultMethod implements PersonaA {

	
	

	@Override
	public void caminar() {
		System.out.println("Hola Coders");
		
	}
	
	
	public static void main(String[] args) {
		DefaultMethod app =new DefaultMethod();
		app.caminar();
		
		
		
		
	}
}
